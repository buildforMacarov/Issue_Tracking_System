{
  "name": "issue-tracking-system",
  "version": "0.0.0",
  "description": "Issue tracking in software from both sides",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js",
    "clean": "rm -rf ./public/dist",
    "prebuild": "npm run clean",
    "build": "webpack",
    "build-watch": "nodemon --exec \"npm run build && npm start\" --ignore \"public/dist\"",
    "reset-testdb": "mysql -u root -p its_test < ./server/tests/its_test.sql",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha server/**/*.test.js",
    "test-reset": "npm run reset-testdb && npm test",
    "test-watch": "nodemon --exec \"npm test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildforMacarov/Issue_Tracking_System.git"
  },
  "keywords": [
    "issue",
    "database",
    "tracking-system"
  ],
  "author": "Spatty Co.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/buildforMacarov/Issue_Tracking_System/issues"
  },
  "homepage": "https://github.com/buildforMacarov/Issue_Tracking_System#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "mysql": "^2.15.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "expect": "^1.20.2",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.2",
    "supertest": "^3.0.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  }
}
